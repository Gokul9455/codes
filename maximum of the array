
class Main {
    public static void main(String[] args) {
        int[]arr={45,24,38,54};
        int max=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        {System.out.println("Maximum element is:"+max);
        }
    }
}
 
