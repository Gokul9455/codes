class Main {
    public static void main(String[] args) {
        int[]arr={45,24,38,54};
        int min=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
        {System.out.println("Minimum element is:"+min);
        }
    }
}
